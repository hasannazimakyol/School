<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog id="dialogNewStudent" title="Adding Student">
		<content>
			<f:SimpleForm id="SimpleFormChange354" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
				labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
				singleContainerFullSize="false">
				<f:content>
					<Label text="Name"/>
						<Input id="inputStudentName"/>
					<Label text="Last Name"/>
						<Input id="inputStudentLastName"></Input>
				</f:content>
			</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="Save" press="onPressSaveStudent" />
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onPressCancelStudent" type="Reject"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>